{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
  "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
  "types": ["jest"]
  },
  "include": ["src", "__tests__"],
  "exclude": ["node_modules", "dist"]
}